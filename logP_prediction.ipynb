import pandas as pd
from rdkit import Chem
from rdkit.Chem import Descriptors

# --- Function to compute LogP using RDKit (Crippen's method) ---
def compute_logp(smiles):
    mol = Chem.MolFromSmiles(smiles)
    if mol:
        return Descriptors.MolLogP(mol)
    else:
        return None

# --- Example list of molecules (SMILES strings) ---
smiles_list = [
    "CCO",        # ethanol
    "CCCC",       # butane
    "CC(=O)O",    # acetic acid
    "c1ccccc1",   # benzene
    "CCN(CC)CC",  # triethylamine
    "CC(C)O"      # isopropanol
]

# --- Compute LogP for each molecule ---
data = pd.DataFrame(smiles_list, columns=["SMILES"])
data["Predicted_LogP"] = data["SMILES"].apply(compute_logp)

print(data)
